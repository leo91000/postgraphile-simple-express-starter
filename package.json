{
  "name": "my-project-here",
  "version": "0.0.0",
  "description": "My description of the project here",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "nodemon --signal SIGINT --watch 'dist/**/*.js' -x \"node --inspect=9678 -r dotenv/config -r source-map-support/register\" dist/index.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "yarn test --watchAll",
    "db:migrate": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS -r dotenv/config\" graphile-migrate migrate",
    "db:watch": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS -r dotenv/config\" graphile-migrate watch",
    "watchAll": "tsc && concurrently --kill-others --names \"TSC,RUN,TEST,DB\" --prefix \"[{name}]\" --prefix-colors \"yellow.bold,cyan.bold,greenBright.bold,purple.bold\" \"tsc --watch --preserveWatchOutput\" \"yarn dev\" \"yarn test:watch\" \"yarn db:watch\""
  },
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "chalk": "^4.1.0",
    "graphile-build": "^4.10.0",
    "graphile-build-pg": "^4.10.0",
    "graphile-migrate": "^1.0.2",
    "graphile-utils": "^4.10.0",
    "helmet": "^4.4.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "postgraphile": "^4.10.0",
    "tslib": "^2.1.0",
    "source-map-support": "^0.5.19",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "cross-env": "^7.0.3",
    "@types/helmet": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/pg": "^7.14.9",
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "graphql": "^15.4.0",
    "jest": "^26.6.3",
    "mock-req": "^0.2.0",
    "typescript": "^4.1.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
